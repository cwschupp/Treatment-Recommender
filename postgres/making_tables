
CREATE TABLE patient
(patient_id int, 
 year_of_birth int, 
 sex char(20), 
 marital_status char(50), 
 smoking_status char(40), 
 ethnic_group char(30), 
 is_cms_patient bool, 
 deceased bool, 
 year_of_death int,
 visible bool, 
 pod varchar);

COPY patient 
	FROM '/Users/clayton.schupp/cwschupp/project/data/sample/patient.tsv' 
	WITH DELIMITER E'\t' CSV HEADER NULL 'NULL';

CREATE TABLE diagnosis
(diagnosis_id int, 
 patient_id int, 
 visit_id int, 
 visit_date timestamp, 
 diagnosis_status char(50), 
 is_new bool, 
 is_ruleout bool, 
 position int, 
 is_existing_with_new_problem bool, 
 complexity_level char(25), 
 default_weight int, 
 diagnosis_name char(255), 
 diagnosis_type char(100), 
 follow_up_unit char(10), 
 follow_up_value int, 
 label_text char(255), 
 icd9 char(50), 
 icd10 char(25), 
 exam_finding bool, 
 medical_domain char(30), 
 special_diagnosis_type char(25), 
 visible bool, 
 pod varchar);

COPY diagnosis 
	FROM '/Users/clayton.schupp/cwschupp/project/data/sample/diagnosis.tsv' 
    WITH DELIMITER E'\t' CSV HEADER NULL 'NULL';

COPY diagnosis 
	FROM '/Users/clayton.schupp/cwschupp/project/data/sample/new_diagnosis.tsv' 
    WITH NULL 'NULL';

CREATE TABLE exam
(visit_id varchar, 
 exam_set varchar, 
 medical_domain varchar, 
 exam_element varchar, 
 exam_system varchar, 
 question varchar, 
 question_type varchar, 
 response_value varchar, 
 range_response_value varchar, 
 pod varchar);

COPY exam FROM '/Users/clayton.schupp/cwschupp/project/data/sample/exam.tsv' 
     WITH DELIMITER E'\t' CSV HEADER NULL 'NULL';



